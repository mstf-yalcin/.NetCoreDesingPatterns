@{
}
@model UserCreateDto

<p>@ViewBag.message</p>


<form asp-controller="Account" asp-action="SignUp">

    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">UserName</label>
        <input asp-for="Name"  class="form-control" >
    </div>

    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Email address</label>
        <input asp-for="Email"  value="test@gmail.com" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Password</label>
        <input name="password" value="Password12*" type="password" class="form-control" id="exampleInputPassword1">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>